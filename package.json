    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^5.2.7",
    "vite-plugin-stylex": "^0.13.0"
  },
  "overrides": {
    "vite-plugin-stylex": {
      "@stylexjs/babel-plugin": "0.4.1"
    }
  }
